  "bin": {
    "cc-editor": "./bin/index.js",
    "cce": "./bin/index.js"
  },
  "scripts": {
    "build-watch": "tsc --watch",
    "test": "tsc && node ./bin/index.js add-editor run",
    "test-help": "tsc && node ./bin/index.js -h",
    "test-format": "tsc && node ./bin/index.js format",
    "test-ccp-set": "tsc && node ./bin/index.js ccp-set -v2 E:/proj -v3 E:/proj",
    "test-use-group": "tsc && node ./bin/index.js use-group",
    "test-ccp-enabled": "tsc && node ./bin/index.js ccp-enabled true",
    "test-setup": "tsc && node ./bin/index.js setup",
    "publish": "tsc && npm publish"
  },
